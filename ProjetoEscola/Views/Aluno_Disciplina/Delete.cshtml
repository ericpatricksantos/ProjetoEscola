@model ProjetoEscola.Models.CRUD.Aluno_Disciplina

@{
    ViewData["Title"] = "Delete";
}

<h1 class="text-center">@ViewData["Title"]</h1>


<div>
   
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.matricula_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.matricula_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.aluno_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.aluno_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.curso_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.curso_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.disciplina_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.disciplina_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.nota)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.nota)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.status)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="matricula_id" />
        <input type="hidden" asp-for="aluno_id" />
        <input type="hidden" asp-for="curso_id" />
        <input type="hidden" asp-for="disciplina_id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
