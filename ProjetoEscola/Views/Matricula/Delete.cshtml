@model ProjetoEscola.Models.Matricula

@{
    ViewData["Title"] = "Deleção de Matrículas";
}

<h1>Deletar Matrícula</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ano)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ano)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.semestre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.semestre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.aluno_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.aluno_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.curso_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.curso_id)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
