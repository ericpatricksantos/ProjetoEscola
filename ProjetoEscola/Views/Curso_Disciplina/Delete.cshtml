@model ProjetoEscola.Models.Entities.Curso_Disciplina

@{
    ViewData["Title"] = "Delete";
}

<h1 class="text-center">@ViewData["Title"]</h1>


<div>
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.data_desativacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.data_desativacao)
        </dd>
    </dl>

